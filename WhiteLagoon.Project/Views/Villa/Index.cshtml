
@model IEnumerable<WhiteLagoon.Domain.Entities.Villa>

<h1>Villa List</h1>

<table class="table table-bordered">
	<thead>
		<tr>
			<th>Name</th>
			<th>Price</th>
			<th>SQFT</th>
		</tr>
	</thead>
	<tbody>
		@foreach(var obj in Model)
		{
			<tr>
				<td> @obj.Name </td>
				<td> @obj.Price</td>
				<td> @obj.Sqft</td>
			</tr>
		}
	</tbody>
</table>

